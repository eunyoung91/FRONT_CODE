<script type="text/javascript">	
	$(function(){
		$(window).on('load', function () {
			pageLoadEvent();
			initAOS();

			$('.visual').addClass('on');
		});
	});

    function pageLoadEvent(){
        var temp_s = $("#premiumWrap").offset().top;
        $('html, body').animate({ scrollTop: temp_s}, 400);
    }

    function initAOS(){
        AOS.init({
            offset: 200,
            duration: 500,
            easing: 'ease-in-sine',
            delay: 0,
            once: true
        });
    }
</script>

